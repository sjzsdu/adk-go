            "args": ["console"],  // ✅ 添加 console 参数启动控制台模式
            "env": {
                "GOOGLE_API_KEY": "your-api-key-here"  // 设置环境变量
            }
        },
        {
            "name": "Launch Quickstart (Directory)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/examples/quickstart",  // ✅ 指向目录
            "console": "integratedTerminal",
            "args": ["console"]
        }
    ]
}